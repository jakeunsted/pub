    "@react-navigation/native": "^7.1.24",
    "@supabase/supabase-js": "^2.86.2",
    "expo": "~54.0.25",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-linking": "~8.0.9",
    "expo-localization": "^17.0.8",
